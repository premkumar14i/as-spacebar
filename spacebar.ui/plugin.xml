   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.tibco.as.spacebar.ui.handlers.metaspace.Delete"
            commandId="com.tibco.as.spacebar.ui.command.metaspace.delete">
         <enabledWhen>
            <with
                  variable="selection">
               <and>
                  <count
                        value="1">
                  </count>
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="com.tibco.as.spacebar.ui.model.Metaspace">
                     </instanceof>
                     <not>
			             <test
			                   property="com.tibco.as.spacebar.ui.model.metaspace.connected">
			             </test>
                     </not>
                  </iterate>
               </and>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="com.tibco.as.spacebar.ui.handlers.metaspace.Connect"
            commandId="com.tibco.as.spacebar.ui.command.metaspace.connect">
         <enabledWhen>
            <with
                  variable="selection">
               <and>
                  <count
                        value="1">
                  </count>
                  <iterate
                        ifEmpty="false"
                        operator="and">
		               <instanceof
        	                   value="com.tibco.as.spacebar.ui.model.Metaspace">
