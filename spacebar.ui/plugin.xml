<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.tibco.as.spacebar.ui.SpaceBarPerspectiveFactory"
            icon="icons/spacebar.png"
            id="com.tibco.as.spacebar.ui.perspective"
            name="ActiveSpaces">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="com.tibco.as.spacebar.ui.perspective">
         <newWizardShortcut
               id="com.tibco.as.spacebar.ui.wizards.metaspace.create">
         </newWizardShortcut>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="com.tibco.as.views"
            class="com.tibco.as.spacebar.ui.navigator.MetaspaceNavigator"
            icon="icons/activespaces.png"
            id="com.tibco.as.spacebar.ui.navigator"
            name="Metaspaces">
      </view>
      <category
            id="com.tibco.as.views"
            name="ActiveSpaces">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            name="Metaspaces"
            priority="low"
            icon="icons/activespaces.png"
            activeByDefault="true"
            contentProvider="com.tibco.as.spacebar.ui.navigator.MetaspaceContentProvider"
            labelProvider="com.tibco.as.spacebar.ui.navigator.MetaspaceLabelProvider"
            id="com.tibco.as.spacebar.ui.navigator.content">
         <enablement>
           <instanceof
                 value="com.tibco.as.spacebar.ui.model.IElement">
           </instanceof>
         </enablement>
         <commonSorter
               class="com.tibco.as.spacebar.ui.navigator.MetaspaceSorter"
               id="com.tibco.as.spacebar.ui.navigator.metaspaceSorter">
         </commonSorter>
         <triggerPoints>
            <instanceof
                  value="com.tibco.as.spacebar.ui.model.Metaspaces">
            </instanceof>
         </triggerPoints>
      </navigatorContent>
      <actionProvider
            class="com.tibco.as.spacebar.ui.navigator.NewActionProvider"
            id="com.tibco.as.spacebar.ui.navigator.NewActionProvider">
         <enablement>
            <or>
               <instanceof
                     value="com.tibco.as.spacebar.ui.model.Metaspace">
               </instanceof>
               <instanceof
                     value="com.tibco.as.spacebar.ui.model.Spaces">
               </instanceof>
               <instanceof
                     value="com.tibco.as.spacebar.ui.model.SpaceFields">
               </instanceof>
               <instanceof
                     value="com.tibco.as.spacebar.ui.model.Indexes">
               </instanceof>
               <adapt
                     type="java.util.Collection">
                  <count
                        value="0">
                  </count>
               </adapt>
            </or>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.tibco.as.spacebar.ui.navigator.ImportExportActionProvider"
            id="com.tibco.as.spacebar.ui.navigator.ImportExportActionProvider">
         <enablement>
            <or>
               <instanceof
                     value="com.tibco.as.spacebar.ui.model.Metaspace">
               </instanceof>
               <instanceof
                     value="com.tibco.as.spacebar.ui.model.Space">
               </instanceof>
               <instanceof
                     value="com.tibco.as.spacebar.ui.model.Spaces">
               </instanceof>
               <adapt
                     type="java.util.Collection">
                  <count
                        value="0">
                  </count>
               </adapt>
            </or>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.tibco.as.spacebar.ui.navigator.PropertiesActionProvider"
            id="com.tibco.as.spacebar.ui.navigator.PropertiesActionProvider">
         <enablement>
            <or>
               <instanceof
                     value="com.tibco.as.spacebar.ui.model.Metaspace">
               </instanceof>
               <instanceof
                     value="com.tibco.as.spacebar.ui.model.Space">
               </instanceof>
               <adapt
                     type="java.util.Collection">
                  <count
                        value="0">
                  </count>
               </adapt>
            </or>
         </enablement>
      </actionProvider>
      <commonWizard
            type="new"
            wizardId="com.tibco.as.spacebar.ui.wizards.metaspace.create">
         <enablement>
            <or>
               <instanceof
                     value="com.tibco.as.spacebar.ui.model.Metaspace">
               </instanceof>
               <adapt
                     type="java.util.Collection">
                  <count
                        value="0">
                  </count>
               </adapt>
            </or>
         </enablement>
      </commonWizard>
      <commonWizard
            type="new"
            wizardId="com.tibco.as.spacebar.ui.wizards.space.create">
         <enablement>
           <instanceof
                 value="com.tibco.as.spacebar.ui.model.Spaces">
           </instanceof>
         </enablement>
      </commonWizard>
      <commonWizard
            type="new"
            wizardId="com.tibco.as.spacebar.ui.wizards.field.add">
         <enablement>
           <instanceof
                 value="com.tibco.as.spacebar.ui.model.SpaceFields">
           </instanceof>
         </enablement>
      </commonWizard>
      <commonWizard
            type="new"
            wizardId="com.tibco.as.spacebar.ui.wizards.index.add">
         <enablement>
           <instanceof
                 value="com.tibco.as.spacebar.ui.model.Indexes">
           </instanceof>
         </enablement>
      </commonWizard>
      <commonWizard
            type="export"
            wizardId="com.tibco.as.spacebar.ui.wizards.export.CSV">
         <enablement>
            <and>
               <instanceof
                     value="com.tibco.as.spacebar.ui.model.Metaspace">
               </instanceof>
               <test
                     property="com.tibco.as.spacebar.ui.model.metaspace.connected">
               </test>
            </and>
         </enablement>
      </commonWizard>
